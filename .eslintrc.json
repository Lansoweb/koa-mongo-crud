{
	"extends": "airbnb-base",
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"require-jsdoc": "off",
		"no-underscore-dangle": ["error", { "allow": ["_id", "_count", "_pageSize", "_total_items", "_page_count"] }],
		"max-len": ["error", { "code": 120 }]
	},
	"globals": {
		"logger": false
	}
}
